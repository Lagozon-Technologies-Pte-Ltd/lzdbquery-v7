[
  {
    "id": "company_mapping_GMPL",
    "document": "Company Mapping: Extended company contact and billing/shipping data.",
    "metadata": {
      "type": "table",
      "primary_key": "Id",
      "join_guidance": [
        {
          "table": "company_GMPL",
          "on": "CompanyId = ID",
          "join_type": "INNER JOIN",
          "optional": false,
          "description": "Mapped to parent company."
        }
      ]
    }
  },
  {
    "id": "tbl_OTCScale_GMPL",
    "document": "OTC Sales: Sale transactions.",
    "metadata": {
      "type": "table",
      "primary_key": "OTCID",
      "join_guidance": [
        {
          "table": "productmaster_GMPL",
          "on": "ProductID = Id",
          "join_type": "INNER JOIN",
          "optional": false,
          "description": "Each sale is for a product."
        },
        {
          "table": "company_mapping_GMPL",
          "on": "CustomerShippingAddress = Id",
          "join_type": "LEFT OUTER JOIN",
          "optional": true,
          "description": "May include shipping address from mapping."
        },
        {
          "table": "company_mapping_GMPL",
          "on": "CompanyBillID = Id",
          "join_type": "LEFT OUTER JOIN",
          "optional": true,
          "description": "May include billing address from mapping."
        }
      ]
    }
  },
  {
    "id": "amazonlisting_GMPL",
    "document": "Amazon Listing: Product mapping to Amazon.",
    "metadata": {
      "type": "table",
      "primary_key": "Id",
      "join_guidance": [
        {
          "table": "productmaster_GMPL",
          "on": "OriginalSku = SKU OR MSKU = MSKU",
          "join_type": "INNER JOIN",
          "optional": false,
          "description": "Maps listing to master product."
        }
      ]
    }
  },
  {
    "id": "tbl_amazon_GMPL",
    "document": "Amazon Orders: Amazon transaction details.",
    "metadata": {
      "type": "table",
      "primary_key": "BillID",
      "join_guidance": [
        {
          "table": "amazonlisting_GMPL",
          "on": "SKU = Sku",
          "join_type": "LEFT OUTER JOIN",
          "optional": true,
          "description": "SKU links to Amazon listing."
        }
      ]
    }
  },
  {
    "id": "af_orderitems_GMPL",
    "document": "Autofurnish Order Items: Line items from Autofurnish orders.",
    "metadata": {
      "type": "table",
      "primary_key": "Id",
      "join_guidance": [
        {
          "table": "autofurnish_orders_GMPL",
          "on": "AFOrderId = OrderNo",
          "join_type": "INNER JOIN",
          "optional": false,
          "description": "Belongs to an Autofurnish order."
        },
        {
          "table": "productmaster_GMPL",
          "on": "Lineitem_sku = SKU",
          "join_type": "LEFT OUTER JOIN",
          "optional": true,
          "description": "SKU links item to product."
        }
      ]
    }
  },
  {
    "id": "inventorydetail_GMPL",
    "document": "Inventory Detail: Inventory movement and tracking.",
    "metadata": {
      "type": "table",
      "primary_key": "Id",
      "join_guidance": [
        {
          "table": "tbl_warehouse_GMPL",
          "on": "WarehouseId = Id",
          "join_type": "LEFT OUTER JOIN",
          "optional": true,
          "description": "Inventory location by warehouse."
        },
        {
          "table": "tbl_rack_GMPL",
          "on": "RackId = Id",
          "join_type": "LEFT OUTER JOIN",
          "optional": true,
          "description": "Inventory storage rack."
        },
        {
          "table": "productmaster_GMPL",
          "on": "MSKU = MSKU",
          "join_type": "LEFT OUTER JOIN",
          "optional": true,
          "description": "Inventory identified via MSKU."
        },
        {
          "table": "statusmaster_GMPL",
          "on": "Status = Id",
          "join_type": "LEFT OUTER JOIN",
          "optional": true,
          "description": "Status of the inventory item."
        }
      ]
    }
  },
  {
    "id": "tbl_rack_GMPL",
    "document": "Rack: Storage rack data.",
    "metadata": {
      "type": "table",
      "primary_key": "Id",
      "join_guidance": [
        {
          "table": "tbl_warehouse_GMPL",
          "on": "WarehouseId = Id",
          "join_type": "INNER JOIN",
          "optional": false,
          "description": "Each rack belongs to a warehouse."
        }
      ]
    }
  }
]